{
  "name": "locale-lists",
  "version": "2.1.0",
  "description": "Lookup locale-related information, such as language, currency, hour format",
  "main": "dist/index.min.js",
  "keywords": [
    "locale",
    "language",
    "country",
    "phone",
    "dial code"
  ],
  "author": "joker876",
  "license": "MIT",
  "files": [
    "dist/index.min.js",
    "dist/*.d.ts"
  ],
  "types": "dist/index.d.ts",
  "scripts": {
    "test": "npm run build && npx jasmine",
    "start": "ts-node-dev --respawn src/index.ts",
    "build": "rmdir /s /Q dist & tsc && terser dist/index.js --output dist/index.min.js --config_file terserconfig.json --comments /^\\*\\n/"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/joker876/locale-lists.git"
  },
  "bugs": {
    "url": "https://github.com/joker876/locale-lists/issues"
  },
  "homepage": "https://github.com/joker876/locale-lists#readme",
  "devDependencies": {
    "@types/node": "^18.7.8",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.7.4",
    "delay": "^5.0.0",
    "@types/jasmine": "^4.0.3",
    "jasmine": "^4.1.0",
    "jasmine-ts-console-reporter": "^3.1.1"
  }
}
